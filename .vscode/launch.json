{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Program",
			"skipFiles": [
				"<node_internals>/**"
			],
			"preLaunchTask": "clean-build",
			"program": "${workspaceFolder}/node_modules/protractor/bin/protractor",
			"args": [
				"${workspaceFolder}/out/conf.js"
			],
			"outFiles": [
				"${workspaceFolder}/out/**/*.js"
			],
			"sourceMaps": true
		}
	]
}